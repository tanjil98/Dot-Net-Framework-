
@{
    ViewBag.Title = "EmployeeList";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}
@Html.ActionLink("Add New Employee", "RegistarEmp", "Admin", null, new { @class = "btn btn-primary" })
<h2 class="text-center">EmployeeList</h2>
<table class="table">
    <thead>
        <tr>
            <th>Employee Id</th>
            <th>Name</th>
            <th>Address</th>
            <th>Gender</th>

            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.EName</td>
                <td>@item.Address</td>
                <td>@item.Gender</td>

                <td>@item.Email</td>
            </tr>
        }
    </tbody>
</table>